<div class="content-wrapper">
    <div class="portfolios-wrapper">
        <div class="portfolios-grid">
          {{#each desImages}}
            <div class="portfolios-grid-item">
                <div class="portfolio-picture-wrapper">
                    <img src="{{ activeImage }}">
                </div>
                <div class="portfolio-picture-text">
                    <p>{{ fullname }}</p>
                </div>
            </div>
          {{/each }}
        </div>
    </div>
</div>
